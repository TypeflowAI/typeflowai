{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "@typeflowai/database#build": {
      "cache": false,
      "dependsOn": ["post-install"]
    },
    "@typeflowai/dashboard#go": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@typeflowai/database#db:setup", "@typeflowai/js#build"]
    },
    "@typeflowai/api#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "@typeflowai/js#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "@typeflowai/workflows#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "@typeflowai/workflows#go": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@typeflowai/workflows#build"]
    },
    "@typeflowai/js#go": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@typeflowai/js#build"]
    },
    "@typeflowai/api#go": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@typeflowai/api#build"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"],
      "env": [
        "AIRTABLE_CLIENT_ID",
        "ASSET_PREFIX_URL",
        "AWS_ACCESS_KEY",
        "AWS_SECRET_KEY",
        "AZUREAD_AUTH_ENABLED",
        "AZUREAD_CLIENT_ID",
        "AZUREAD_CLIENT_SECRET",
        "AZUREAD_TENANT_ID",
        "DATABASE_URL",
        "DEFAULT_TEAM_ID",
        "DEFAULT_TEAM_ROLE",
        "ONBOARDING_DISABLED",
        "CRON_SECRET",
        "DEBUG",
        "EMAIL_VERIFICATION_DISABLED",
        "ENCRYPTION_KEY",
        "TYPEFLOWAI_ENCRYPTION_KEY",
        "GITHUB_ID",
        "GITHUB_SECRET",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET",
        "GOOGLE_SHEETS_CLIENT_ID",
        "GOOGLE_SHEETS_CLIENT_SECRET",
        "GOOGLE_SHEETS_REDIRECT_URL",
        "NOTION_OAUTH_CLIENT_ID",
        "NOTION_OAUTH_CLIENT_SECRET",
        "HEROKU_APP_NAME",
        "IMPRINT_URL",
        "INSTANCE_ID",
        "INTERNAL_SECRET",
        "INVITE_DISABLED",
        "IS_TYPEFLOWAI_CLOUD",
        "GOOGLE_AUTH_ENABLED",
        "GITHUB_AUTH_ENABLED",
        "OPENAI_SECRET_KEY",
        "MAIL_FROM",
        "NEXT_PUBLIC_DOCSEARCH_APP_ID",
        "NEXT_PUBLIC_DOCSEARCH_API_KEY",
        "NEXT_PUBLIC_DOCSEARCH_INDEX_NAME",
        "NEXT_PUBLIC_TYPEFLOWAI_API_HOST",
        "NEXT_PUBLIC_TYPEFLOWAI_ENVIRONMENT_ID",
        "NEXT_PUBLIC_TYPEFLOWAI_ONBOARDING_WORKFLOW_ID",
        "NEXT_PUBLIC_TYPEFLOWAI_PMF_FORM_ID",
        "NEXT_PUBLIC_TYPEFLOWAI_URL",
        "NEXT_PUBLIC_SENTRY_DSN",
        "NEXT_PUBLIC_POSTHOG_API_HOST",
        "NEXT_PUBLIC_POSTHOG_API_KEY",
        "NEXT_PUBLIC_TYPEFLOWAI_COM_API_HOST",
        "NEXT_PUBLIC_TYPEFLOWAI_COM_ENVIRONMENT_ID",
        "NEXT_PUBLIC_TYPEFLOWAI_COM_DOCS_FEEDBACK_WORKFLOW_ID",
        "NEXTAUTH_SECRET",
        "NEXTAUTH_URL",
        "NODE_ENV",
        "PASSWORD_RESET_DISABLED",
        "PRIVACY_URL",
        "RATE_LIMITING_DISABLED",
        "S3_ACCESS_KEY",
        "S3_SECRET_KEY",
        "S3_REGION",
        "S3_BUCKET_NAME",
        "SENTRY_DSN",
        "SHORT_URL_BASE",
        "SIGNUP_DISABLED",
        "SMTP_HOST",
        "SMTP_PASSWORD",
        "SMTP_PORT",
        "SMTP_SECURE_ENABLED",
        "SMTP_USER",
        "STRIPE_SECRET_KEY",
        "STRIPE_WEBHOOK_SECRET",
        "WORKFLOWS_PACKAGE_MODE",
        "WORKFLOWS_PACKAGE_BUILD",
        "TELEMETRY_DISABLED",
        "TERMS_URL",
        "VERCEL_URL",
        "WEBAPP_URL"
      ]
    },
    "post-install": {
      "cache": false,
      "dependsOn": [],
      "outputs": [],
      "inputs": ["./schema.prisma"]
    },
    "go": {
      "persistent": true,
      "cache": false
    },
    "db:migrate:dev": {
      "outputs": []
    },
    "db:migrate:deploy": {
      "outputs": []
    },
    "db:migrate:prod": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "generate": {
      "dependsOn": ["^generate"]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "outputs": []
    }
  }
}
