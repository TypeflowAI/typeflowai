{
  "name": "@typeflowai/workflows",
  "license": "MIT",
  "version": "1.0.1",
  "description": "TypeflowAI-workflows is a helper library to embed workflows into your application",
  "homepage": "https://typeflowai.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/TypeflowAI/typeflowai"
  },
  "sideEffects": false,
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "dev": "WORKFLOWS_PACKAGE_MODE=development vite build --watch",
    "build": "pnpm run build:workflows && pnpm run build:question-date",
    "build:workflows": "tsc && WORKFLOWS_PACKAGE_BUILD=workflows vite build",
    "build:question-date": "tsc && WORKFLOWS_PACKAGE_BUILD=question-date vite build",
    "go": "concurrently \"pnpm dev\" \"serve dist -p 3003\"",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "preview": "vite preview",
    "clean": "rimraf .turbo node_modules dist"
  },
  "devDependencies": {
    "@calcom/embed-snippet": "1.1.2",
    "@preact/preset-vite": "^2.8.1",
    "@typeflowai/api": "workspace:*",
    "@typeflowai/lib": "workspace:*",
    "@typeflowai/tsconfig": "workspace:*",
    "@typeflowai/types": "workspace:*",
    "autoprefixer": "^10.4.16",
    "concurrently": "8.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "latest",
    "postcss": "^8.4.32",
    "preact": "^10.19.3",
    "react-date-picker": "^10.6.0",
    "serve": "14.2.1",
    "tailwindcss": "^3.4.0",
    "terser": "^5.26.0",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.0",
    "vite-tsconfig-paths": "^4.2.3"
  },
  "dependencies": {
    "eventsource-parser": "^1.1.1",
    "marked": "^12.0.2",
    "preact-markdown": "^2.1.0"
  }
}
