{
  "name": "@typeflowai/workflows",
  "license": "MIT",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "TypeflowAI-workflows is a helper library to embed workflows into your application",
  "homepage": "https://typeflowai.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/TypeflowAI/typeflowai"
  },
  "sideEffects": false,
  "source": "src/index.ts",
  "main": "dist/index.umd.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite build --watch --mode dev",
    "serve": "serve dist -p 3003",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode dev",
    "go": "vite build --watch --mode dev",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "preview": "vite preview",
    "clean": "rimraf .turbo node_modules dist"
  },
  "devDependencies": {
    "@calcom/embed-snippet": "1.2.0",
    "@typeflowai/api": "workspace:*",
    "@typeflowai/lib": "workspace:*",
    "@typeflowai/tsconfig": "workspace:*",
    "@typeflowai/types": "workspace:*",
    "@preact/preset-vite": "^2.8.2",
    "autoprefixer": "^10.4.19",
    "concurrently": "8.2.2",
    "eslint-config-typeflowai": "workspace:*",
    "isomorphic-dompurify": "^2.9.0",
    "postcss": "^8.4.38",
    "preact": "^10.21.0",
    "react-date-picker": "^11.0.0",
    "serve": "14.2.3",
    "tailwindcss": "^3.4.3",
    "terser": "^5.31.0",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "eventsource-parser": "^1.1.1",
    "marked": "^12.0.2",
    "preact-markdown": "^2.1.0"
  }
}
