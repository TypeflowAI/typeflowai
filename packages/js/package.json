{
  "name": "@typeflowai/js",
  "license": "MIT",
  "version": "1.0.0",
  "description": "TypeflowAI-js allows you to connect your app to TypeflowAI, display workflows and trigger events.",
  "homepage": "https://typeflowai.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/TypeflowAI/typeflowai"
  },
  "keywords": [
    "TypeflowAI",
    "workflows",
    "experience management"
  ],
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "unpkg": "./dist/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "go": "vite build --watch",
    "lint": "eslint ./src --fix",
    "test": "jest --coverage --no-cache",
    "clean": "rimraf .turbo node_modules dist coverage"
  },
  "author": "TypeflowAI <support@typeflowai.com>",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@typeflowai/api": "workspace:*",
    "@typeflowai/lib": "workspace:*",
    "@typeflowai/workflows": "workspace:*",
    "@typeflowai/tsconfig": "workspace:*",
    "@typeflowai/types": "workspace:*",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "terser": "^5.26.0",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.6.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "latest"
  },
  "jest": {
    "transformIgnorePatterns": [
      "!node_modules/"
    ],
    "setupFiles": [
      "<rootDir>/tests/__mocks__/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/tests/__mocks__/styleMock.js"
    }
  }
}
